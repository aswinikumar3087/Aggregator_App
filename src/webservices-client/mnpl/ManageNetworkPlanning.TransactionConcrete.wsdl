			<xsd:complexType name="NetworkPlanningActivity">
				<xsd:annotation>
					<xsd:documentation>The NetworkPlanningActivity entity helps define the various activities that need to be stored in P and B for every NetworkProject. e.g. DSLAM planning, NGA Exchange Plan and Build, Site Survey etc </xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="activityId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Key generated within Plan and Build platform to uniquely identify an activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activitydescription" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Some description associated with the activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Type of Activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityCategory" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the Category. It is the category of the journey or project</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="subCategory" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the sub Category of an activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityStatus" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the status of the activity.  e.g. Open, Closed, Activated, Suspended, Canceled</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="priority" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the priority of the activity. e.g. Low, Very Low, Medium, High, Very High</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="criticalFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Whether the activity is critical for the journey or project. e.g. Y or N</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="milestoneFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Whether the activity is a milestone activity for the  journey or project. e.g. Y or N</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="parentId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier of the parent project e.g. EAD00201</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="plannedStartDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Activity Start Date i.e. date the task was triggered.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="plannedFinishDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Activity Finish Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="startDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Planned Activity Start Date i.e. date the Activity was triggered.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="finishDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Planned Activity Finish Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="baselineDuration" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Duration with respect to project</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lastUpdatedBy" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>EIN of the person who last updated the activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lastUpdatedDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Date when details were last updated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityIssuedBy" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>EIN of the person who issued the activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityNote" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Any notes related to activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="contactInfo" type="mnpltxn:NetworkContact" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>This can contain for example ID of the person to whom the task has been assigned etc. Refers to PBP EDM Entity - NetworkContact (0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="activityCharges" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>To capture charges associated with network items</xsd:documentation>
