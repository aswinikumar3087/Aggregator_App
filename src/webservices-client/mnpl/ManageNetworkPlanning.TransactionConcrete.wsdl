							<xsd:documentation>Type of Build specifically for the Network entity like Headend, Incremental Build, Infield build etc.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="startDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Project Start Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="endDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Project End Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="requestedBy" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier of the person who requested the project to be created.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="requestedTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The time when the project creation was requested</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="projectFormat" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Might be a redundant field, no registered usage found.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="programmeName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of where the project sits in the programme hierarchy</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="managerResourceId" type="xsd:int" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Manager's EIN who owns the project</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="phase" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A build is divided in multiple Phases, used to determine a particular phase in a build.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="size" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines how big the project is: V Small, Small, Medium, Large or V Large</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="action" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used  to identify the type of operation eg: Cancel/amend/suspend</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="updateReason" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Document the reason for updating a Network Project. Refers to PBP KVP entity (Cardinality - 						0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="projectMilestone" type="mnpltxn:ProjectMilestone" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>A value business uses to represent a period of time, between two timepoints to allow the business to:compare the time interval with a point of time and compare time intervals</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="scheme" type="mnpltxn:Scheme" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP EDM Entity - Scheme (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="orderData" type="mnpltxn:NetworkOrder" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP EDM Entity - NetworkOrder (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="subNP" minOccurs="0">
						<xsd:complexType>
							<xsd:choice>
								<xsd:element ref="mnpltxn:subNetworkProjectRefs" minOccurs="0"/>
								<xsd:element name="subNetworkProjects" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="subNetworkProject" type="mnpltxn:NetworkProject" minOccurs="0" maxOccurs="unbounded">
												<xsd:annotation>
													<xsd:documentation>Refers to PBP EDM Entity - NetworkProject (Cardinality - 0..*)</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:choice>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="planningActivity" type="mnpltxn:NetworkPlanningActivity" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP EDM Entity - NetworkPlanningActivity (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="contact" type="mnpltxn:NetworkContact" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP EDM Entity - NetworkContact (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="location" type="mnpltxn:NetworkSite" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP EDM Entity - NetworkSite (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="person" type="ns1:Person" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to EMP EDM Entity - Person (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="customerAppointmentList" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="customerAppointment" type="ns2:CustomerEngineeringAppointment" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers EWMP entity CustomerEngineeringAppointment (Cardinality - 0..*) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="note" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Capture project related notes. Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="projectRelatedFlags" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This structure helps group all types of flags required at the NetworkProject level</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="chargeApplicableFlag" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Indicates whether charges are applicable for this project.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="progressToBuildFlag" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Represents "Go Ahead" from the consumer for project progression.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="cableLinkRequiredFlag" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Indicates whether a cable link is required.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="DPDiscrepancyFoundFlag" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Indicates whether a discrepancy was found at DP.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="spineDuctRequiredFlag" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Indicates whether further network planning is required by planner.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="actionRequiredDetails" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>These set of atttributes give details about the actions that would be triggered at the consumer end as a result of various activities performed during project orchestration.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="code" type="xsd:string" minOccurs="0"/>
								<xsd:element name="text" type="xsd:string" minOccurs="0"/>
								<xsd:element name="timeStamp" type="xsd:dateTime" minOccurs="0"/>
								<xsd:element name="person" type="ns1:Person" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to EMP EDM Entity - Person (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="prjUpdateHistory" type="mnpltxn:NetworkProjectUpdateHistory" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Capture the network project, network task and network order related transaction data. Refers to PBP EDM entity NetworkProjectUpdateHistory (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="NetworkProjectUpdateHistory">
				<xsd:annotation>
					<xsd:documentation>This entity helps capture the network project, network task and network order related transaction data.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="transactionId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction id</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transactionName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transactionType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction Type</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transactionAction" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Transaction Action</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="actionName" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Example values : Submitted, Completed</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="transactionData" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="transactionStartDateTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction start Date and Time</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transactionEndDateTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction End Date and Time</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="person" type="ns1:Person" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Refers to EMP EDM entity Person (Cardinality - 0..1) for capturing the User/BT UIN of the person who has initiated the transaction</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="NetworkResourceLocation">
				<xsd:annotation>
					<xsd:documentation>This entity identifies the various resource locations within a network site e.g. rooms that house equipments.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="friendlyName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Friendly name for a resource location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="description" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Description for a resource location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="type" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Type if any - need to check on examples </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="nadALK" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>NAD ALK is the unique identifier created by PIPeR for the location of a physical resource e.g. physical structure. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="alternateID" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This structure is used to accomodate any identifiers for resource locations other than the ones defined in the model.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="id" type="mnpltxn:KVP" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Key value pair for Alternate ids for resoure locations e.g. key=BFD_ID and value=actual_value. Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="trsGroup" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Localities are representation of an area or part of a building or a town. A locality is represented by 1141 code. A locality may have many TRS Groups. TRS stands for Transmit and Receive Sections </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="trsArea" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Each TRS Group may have many TRS Areas. TRS Areas are the physical location where the network elements are placed and they are also the termination points for cables etc..TRS stands for Transmit and Receive Sections</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="unvalidatedAddress" type="mnpltxn:UnvalidatedAddress" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Contains address which is yet not available in an external address source such as PAF, AL2 etc</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="code1141" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>1141 code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="floor" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier or name for a floor at a network site</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="section" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier or name for a section at a network site</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="section_1141Code" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>1141 code of section</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="room" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier or name for a room at a network site</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="room_1141Code" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>1141 code of room</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ndp" type="mnpltxn:NetworkDeliveryPoint" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Contains information about the actual location where an equipement is located within a NetworkResourceLocation eg. suite, rack, shelf, slot information. Refers to PBP EDM entity NetworkDeliveryPoint (Cardinality - 0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="position" type="mnpltxn:Position" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Captures the co-ordinates such as x,y co-ordinates, northings and eastings etc. Refers to PBP EDM entity Position (Cardinality - 0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="engineeringInfo" type="mnpltxn:EngineeringInfoForLocation" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This entity captures information about hazard and warning, permit to work, TMA related attributes. This refers to PBP EDM entityEngineeringInfoForLocation (Cardinality - 0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="additionalInfoList" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Capture additional information as key-value pair if not present in the model. </xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="additionalInfo" type="mnpltxn:KVP" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="NetworkResourceService">
				<xsd:sequence>
					<xsd:element name="serviceIdentifier" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier for the service.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Service Name.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="serviceType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Type of the service</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="description" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Service Description</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="priority" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Priority of the service</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="status" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>e.g. In Service</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="circuitStatus" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Status of the circuit</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="servicePackageID" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>   Package ID that groups circuits together Implemented only for VLANs associated with IPStream SVLAN BB IPStream
    WESB circuits associated with WESB RO2,WESB RO3 service package
    ONBS circuits associated with ONBS(RO2), ONBS(RO3) service package
    ETBH circuits associated with ETBH RO2, ETBH RO3 service package --(Not Yet Implemented )
    Voice Media and Voice Signalling circuits associated with BFD service package</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="serviceFulfilmentFlag" type="xsd:boolean" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates the service fulfilment status of the service which is provided to the customer.Source: MSR Q3, 2007</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="definitelyAffectedFlag" type="xsd:boolean" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Flag to indicate whether the service instance is definitely affected.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="QOS" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Quality of Service Indicator. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="productType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Product Type</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RTSFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>RTS Flag is to indicate whether the network port is to be added as the sync reference</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LTSFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>LTS Flag is to indicate whether the line card would carry the timing source.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="retainSyncFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Retain Sync flag is to indicate whether a service is already configured on the chassis with a Sync input option</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="firstSyncOrderFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>It is to indicate that this is the first Sync order on the chassis.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="serviceCharacteristics" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="serviceCharacteristic" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="serviceDates" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="serviceDate" type="mnpltxn:ResourceDate" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity ResourceDate (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="orderRef" type="mnpltxn:NetworkOrder" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP EDM Entity - NetworkOrder (Cardinality - 0..*) </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="aliasNames" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="aliasName" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="NetworkSite">
				<xsd:annotation>
					<xsd:documentation>Any bounded place of interest to BT which houses network equipments . eg. exchange. Each NetworkSite may be associated with a postal address mastered by SSP. </xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="siteId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Unique id assigned to every site within P and B </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the network site </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="description" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A description to provide further information about the network site </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="siteType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>E.g. TemporarySite, BT_Exchange </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="status" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This field can be converted into an enumeration which states whether the site is functional, defunct, planned etc</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressRef" type="ns4:Address" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> This refers to SSP Address entity (Cardinality - 0..1) </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="unStructuredAddress" type="ns4:UnstructuredAddress" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Refers to UnstructuredAddress entity defined by SSP Cardinality (0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="building" type="mnpltxn:Building" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The Building entity will be created in case of multi occupancy structures specifically to capture entities which do not have a postal address e.g. service lifts.  This refers to PBP EDM entity Building (Cardinality - 0..1) </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="nadKey" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>NAD key is the unique identifier assigned by SSP for an address </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="friendlyName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This atttribute is used to capture location details for sites which do not have a postal address e.g. At junction of A168 and A19 where the Network Site is traffic lights. This attribute should not be used for address details </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="alternateID" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This structure is used to accomodate any identifiers for sites other than the ones defined in the model.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="id" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Key value pair for Alternate ids for sites. Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="siteLanguage" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Language spoken at the network site</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="exchangeName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the exchange switching unit e.g. from CSS CWMBRAN UC SYSX</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="exchangeCode" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Code for an exchange e.g. CRSUC</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="exchangeType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Exchange Type e.g. NDXD03</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="exchange1141Code" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>1141 code for an exchange. Usually made up of the exchangeCode, site code and unit code. e.g. CRS/UC </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="districtID" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>CSS is divided into 29 districts. This attribute is used to uniquely identify the district. Each district has some exchanges within it. Included this attribute in the EDM model since a lot of interfaces exchange this attribute.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="districtName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>CSS is divided into 29 districts. This attribute holds the name of the district. Each district has some exchanges within it. Included this attribute in the EDM model since a lot of interfaces exchange this attribute.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="skylineCode" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Skyline Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="position" type="mnpltxn:Position" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Captures the co-ordinates such as x,y co-ordinates, northings and eastings etc. This refers to PBP EDM entity Position (Cardinality - 0..1)  </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="engineeringInfo" type="mnpltxn:EngineeringInfoForLocation" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This entity captures information about hazard and warning, permit to work, TMA related attributes. This refers to PBP EDM entity EngineeringInfoForLocation (Cardinality - 0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="resourceLocation" type="mnpltxn:NetworkResourceLocation" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Identify the various resource locations within a network site. e.g. rooms that house equipments. This refers to PBP EDM entity NetworkResourceLocation (Cardinality - 0..*)  </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="region" type="xsd:string" minOccurs="0"/>
					<xsd:element name="siteNotes" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Site notes can be used to capture specific details about a site</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="note" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="usrn" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A unique identifier for each street as per the National Street Gazetteer (NSG) . </xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="usrn" type="xsd:integer" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Positive integer in the range 1- 99999999 Max Length :8 </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="addressRef" type="ns4:Address" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation> This refers to SSP Address entity (Cardinality - 0..1) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="NetworkTask">
				<xsd:annotation>
					<xsd:documentation>The NetworkTask entity helps define the various tasks that need to be stored in P and B for every NetworkPlanningActivity. </xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="taskId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Key generated within Plan and Build platform to uniquely identify a task e.g. EAD00110, EAD00120</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the task. e.g. Update NAD Key, Amend Site Details</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskdescription" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Some description associated with the task</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Type of Task e.g. Manual or Auto </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskCategory" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the Category. It is the category of the journey or project e.g PLANDPOLT</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="subCategory" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the sub Category of a task.e.g MIXED</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskStatus" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the status of the task. e.g. Open, Closed, Activated, Suspended, Canceled</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskPriority" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Defines the priority of the task. e.g. Low, Very Low, Medium, High, Very High</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="criticalFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Whether the task is critical for the journey or project. e.g. Y or N</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="milestoneFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Whether the task is a milestone task for the  journey or project. e.g. Y or N</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="parentId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifier of the parent activity or project e.g. EAD00201</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="plannedStartDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Task Start Date i.e. date the task was triggered.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="plannedFinishDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Task Finish Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="startDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Planned Task Start Date i.e. date the task was triggered.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="finishDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Planned Task Finish Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="baselineDuration" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Duration with respect to project</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lastUpdatedBy" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>EIN of the person who last updated the task</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="lastUpdatedDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Date when details were last updated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskIssuedBy" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>EIN of the person who issued the task</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskNote" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Any notes related to task.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="contactInfo" type="mnpltxn:NetworkContact" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>This can contain for example ID of the person to whom the task has been assigned etc. Refers to PBP EDM Entity - NetworkContact (0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="location" type="mnpltxn:NetworkSite" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Location details for a NetworkTask. Refers to PBP EDM Entity - NetworkSite (Cardinality - 0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskCharges" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>To capture charges associated with network items</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="chargeItem" type="mnpltxn:NetworkCharge" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM Entity - NetworkCharge (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="modifiedProjectData" type="mnpltxn:NetworkProject" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This is used for the data that is modifed at each task level. Refers to PBP EDM entity - NetworkProject (0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskDuration" type="xsd:integer" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This is used to define the task duration. e.g. number of days to complete the task.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="customerAppointmentList" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="customerAppointment" type="ns2:CustomerEngineeringAppointment" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers EWMP entity CustomerEngineeringAppointment (Cardinality - 0..*) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="actualTaskDuration" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This is used to define the actual task duration. e.g. number of hours and mins to complete the task.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="estimatedTaskDuration" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This is used to define the estimated task duration. e.g. number of hours and mins to complete the task.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="additionalInfoList" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Capture additional task related data</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="additionalInfo" type="mnpltxn:KVP" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="dependentTaskId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Id of the task on which this task is dependent on.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="dependentTaskName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the task on which this task is dependent on.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="calledOffStatusFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates whether the SOD Payments task is ready. Permissible values are Y or N, Yes or No, True or False.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="calledOffTimeStamp" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Time at which Call off is recorded in system</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="readyForSODPaymentFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates whether the network task, SOD Payments, is ready for any further processing. Permissible values are Y or N, Yes or No, True or False.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="readyForSODPaymentTimeStamp" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Time at which Ready for Payment recorded in the system.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SODPaymentStatusFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Permissible values are Snagging, PR Generated, Approved, Rejected, Requested.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SODRequestRefNo" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>This is a unique id generated to identify SOD payment type of task in addition to the task id that is generated.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taskTriggeredFrom" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>States whether request is raised from NewSites or NSM (i.e. OR Portal)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="QG7StatusFlag" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Status of the Quality Gate 7 task. Valuea are Pass or Fail, P or F</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="QG7StatusTimeStamp" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Time at which Quality Gate 7 status is recorded in the system</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SODPaymentMode" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Plot payment mode. e.g. USO,NON-USO With Payment, USO With Payment, NON-USO Without Payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SODStatusChangeTimeStamp" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Timestamp to document the status change of SOD.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SODRequestTimeStamp" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Timestamp to document when the SOD request was made.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SODPRDetails" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Refers the EMP Entity Requisition to capture the details of PR raised by this task. (Cardinality 0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PRChangeTimeStamp" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Timestamp to document when the SOD request was made.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="networkTaskSnagInfo" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Captures the snags encountered for this particular network task</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="networkTaskSnagInfo" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity NetworkTaskSnag (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:simpleType name="ObjectTypeEnum">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="AID"/>
					<xsd:enumeration value="ASA"/>
					<xsd:enumeration value="ASAP"/>
					<xsd:enumeration value="CTP"/>
					<xsd:enumeration value="EH"/>
					<xsd:enumeration value="EPG"/>
					<xsd:enumeration value="EQ"/>
					<xsd:enumeration value="FD"/>
					<xsd:enumeration value="FDFR"/>
					<xsd:enumeration value="FTP"/>
					<xsd:enumeration value="GTP"/>
					<xsd:enumeration value="MD"/>
					<xsd:enumeration value="ME"/>
					<xsd:enumeration value="MFD"/>
					<xsd:enumeration value="MLSN"/>
					<xsd:enumeration value="OS"/>
					<xsd:enumeration value="PG"/>
					<xsd:enumeration value="PMP"/>
					<xsd:enumeration value="PTP"/>
					<xsd:enumeration value="SNC"/>
					<xsd:enumeration value="TCAPP"/>
					<xsd:enumeration value="TCPROFILE"/>
					<xsd:enumeration value="TL"/>
					<xsd:enumeration value="TMD"/>
					<xsd:enumeration value="TPPOOL"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType name="PhysicalTerminationPoint">
				<xsd:complexContent>
					<xsd:extension base="mnpltxn:TerminationPoint">
						<xsd:sequence>
							<xsd:element name="physicalPortName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="logicalPortName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="autoNegotiation" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Whether the Port is Auto Negotiating or not Also used for InventoryConfiguration, to instruct the DAALI to set Auto-Negotiation or not. Possible Values are ON and OFF</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tamBlockingGroup" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Only required by EVOTAM Test Access Matrix ManagedElement. Circuits on the same TAM in the same blocking group cannot be routined (tested) at the same time (they are blocking). Each blocking group is identified by a separate letter A/B/C/D. The circuit blocking group can be identified from the circuit number (1..200). A is 1,5,9 etc B is 2,6,10 etc ... How the blocking group relates to the EvoTAM port is TBA by Ajay Bahjne... I think blocking groups are probably done by separate test heads. RFC2222 says "the two numbers are chain and subrack and the letter is the blocking group" I do not know what this signifies.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="testHeadIPPort" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>The test Head IP Port.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
			<xsd:simpleType name="PlanningDateTypeEnum">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ACTUAL_DATE"/>
					<xsd:enumeration value="FORECAST_DATE"/>
					<xsd:enumeration value="EXPECTED_DATE"/>
					<xsd:enumeration value="REQUESTED_DATE"/>
					<xsd:enumeration value="LAST_MODIFIED_DATE"/>
					<xsd:enumeration value="COMMISSIONED_DATE"/>
					<xsd:enumeration value="INSPECTION_DATE"/>
					<xsd:enumeration value="AGREED_DATE"/>
					<xsd:enumeration value="START_DATE"/>
					<xsd:enumeration value="END_DATE"/>
					<xsd:enumeration value="CANCELLED_DATE"/>
					<xsd:enumeration value="REJECTED_DATE"/>
					<xsd:enumeration value="UNKNOWN"/>
					<xsd:enumeration value="NA"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType name="Position">
				<xsd:annotation>
					<xsd:documentation>A choice of position coordinates/references can be used. NOTE more than one can be used.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="geodeticCoordinate" minOccurs="0">
						<xsd:complexType>
							<xsd:annotation>
								<xsd:documentation>Represents the details of the US National Geodetic Survey (GPS) coding system.</xsd:documentation>
							</xsd:annotation>
							<xsd:sequence>
								<xsd:element name="latitude" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>A component part (with Longitude) of the US National Geodetic Survey (GPS) reference which uniquely identifies a point in relation to the earth s surface, eg 52 01 40.070N. Source: Capability Design (Manage Place)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="longitude" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>A component part (with Latitude) of the US National Geodetic Survey (GPS) reference which uniquely identifies a point in relation to the earth s surface, eg 01 04 57.067E. Source: Capability Design (Manage Place)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ellipoidalHeight" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Source: Manage Place</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="localGridCoordinateSystem" minOccurs="0">
						<xsd:complexType>
							<xsd:annotation>
								<xsd:documentation>Applies to Sub-locations, eg in order to pinpoint where in an exchange room a frame might be placed and its orientation.</xsd:documentation>
							</xsd:annotation>
							<xsd:sequence>
								<xsd:element name="x" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>The distance from the yz plane. Source: Capability Design (Manage Place) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="y" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>The distance from the zx plane. Source: Capability Design (Manage Place)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="z" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>The distance from the xy plane. Source: Capability Design (Manage Place) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="gridSystem" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>The value can be one of easting-and-northing and latitude-and-longitude. Source: MSR, Q3, 2007</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="osNationalGridCoordinate" minOccurs="0">
						<xsd:complexType>
							<xsd:annotation>
								<xsd:documentation>The National Grid is designed to cover the whole of Great Britain and extends 700 km to the east and 1300 km to the north of the false origin. Within this framework we can use a National Grid reference to state the position of any point in Britain, to any level of precision that we require. Essentially, we need to state two numbers: an easting (along the horizontal axis) and a northing (along the vertical axis). Source:Ordnance Surevy at http://www.gps.gov.uk/natgrid/page5.asp</xsd:documentation>
							</xsd:annotation>
							<xsd:sequence>
								<xsd:element name="northing" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>A component part (with Easting) of the Ordnance Survey National Grid (GPS) reference which uniquely identifies a point on the earth s surface, eg 241360.002 on the vertical axis of the grid. Source:Capability Design (Manage Place)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="easting" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>A component part (with Northing) of the Ordnance Survey National Grid (GPS) reference which uniquely identifies a point on the earth s surface, eg 611550.007 on the horizontal axis of the grid.Source: Capability Design (Manage Place)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="orthometricHeight" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Distance above or below the earth s surface. Source: Capability Design (Manage Place) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="position" minOccurs="0">
						<xsd:complexType>
							<xsd:annotation>
								<xsd:documentation>Represents the details of the Ordnance Survey grid reference coding system.</xsd:documentation>
							</xsd:annotation>
							<xsd:sequence>
								<xsd:element name="gridReference" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>The Ordnance Survey Landranger grid reference which uniquely identifies a point on the earth s surface, eg TM11554136.
Source: Capability Design (Manage Place)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="ProjectMilestone">
				<xsd:sequence>
					<xsd:element name="milestoneId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Project Milestone id</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Milestone name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="description" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Milestone description</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="startDateTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Start Date and Time</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="endDateTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>End Date and Time</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="baselineEndDate" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Baseline End Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="RelativeDistinguishNameType">
				<xsd:annotation>
					<xsd:documentation>This is the type definition of relative distinguish name (RDN) data structure used in all object names.</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="type" type="mnpltxn:ObjectTypeEnum">
						<xsd:annotation>
							<xsd:documentation>RDN object type acronym. Refers PBP Enumeration ObjectTypeEnum</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="value" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>RDN value</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="ResourceDate">
				<xsd:sequence>
					<xsd:element name="dateType" type="mnpltxn:PlanningDateTypeEnum">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP Enumeration PlanningDateTypeEnum</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="resourceState" type="mnpltxn:ResourceStateEnum" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Refers to PBP Enumeration ResourceStateEnum</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="dateTime" type="xsd:dateTime"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:simpleType name="ResourceStateEnum">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="INSTALLED"/>
					<xsd:enumeration value="PLANNED"/>
					<xsd:enumeration value="PROP_COMMISSIONING"/>
					<xsd:enumeration value="PROP_DECOMMISSIONING"/>
					<xsd:enumeration value="PROP_FUNDAMENTAL_PLANNING"/>
					<xsd:enumeration value="PROP_SOLUTION_DESIGN"/>
					<xsd:enumeration value="PROP_STATUS"/>
					<xsd:enumeration value="PROP_INITIAL"/>
					<xsd:enumeration value="PROP_PLANNING"/>
					<xsd:enumeration value="PROP_RETIRING_ACTIVE"/>
					<xsd:enumeration value="PROP_WIRED"/>
					<xsd:enumeration value="PROP_RESERVED"/>
					<xsd:enumeration value="RETIRED"/>
					<xsd:enumeration value="RETIRING"/>
					<xsd:enumeration value="INSTALLING_ACCEPTED"/>
					<xsd:enumeration value="INSTALLING_COMMISSIONED"/>
					<xsd:enumeration value="INSTALLING_DELIVERED"/>
					<xsd:enumeration value="INSTALLING_INSTALLED"/>
					<xsd:enumeration value="INSTALLING_INTEGRATED"/>
					<xsd:enumeration value="INSTALLING_REJECTED"/>
					<xsd:enumeration value="NON_WORKING"/>
					<xsd:enumeration value="PLANNING"/>
					<xsd:enumeration value="PLANNING_INITIAL_PLAN"/>
					<xsd:enumeration value="PLANNING_ORDERED"/>
					<xsd:enumeration value="PLANNING_PLANNED"/>
					<xsd:enumeration value="RETIRING_DECOMMISSIONED"/>
					<xsd:enumeration value="RETIRING_DEINSTALLED"/>
					<xsd:enumeration value="RETIRING_DEINTEGRATED"/>
					<xsd:enumeration value="RETIRING_RECOVERED"/>
					<xsd:enumeration value="RETIRING_STORED"/>
					<xsd:enumeration value="RETIRING_WITHDRAWN_ACTIVE"/>
					<xsd:enumeration value="RETIRING_WITHDRAWN_UNAVAILABLE"/>
					<xsd:enumeration value="UNKNOWN"/>
					<xsd:enumeration value="NA"/>
					<xsd:enumeration value="WORKING"/>
					<xsd:enumeration value="WORKING_ACTIVATED"/>
					<xsd:enumeration value="WORKING_DEACTIVATED"/>
					<xsd:enumeration value="ALLOCATED"/>
					<xsd:enumeration value="COMPLETION"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType name="Scheme">
				<xsd:annotation>
					<xsd:documentation>This entity captures details about a template to be followed for a network project or network job or network task</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="schemeId" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Unique identifier for the scheme or template</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the scheme</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="type" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Type of scheme</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="status" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Status of the scheme e.g. InUse, Deprecated etc</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="schemeCategory" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Scheme Category</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fistNumber" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used for progressing Schemes e.g. 038AH ERERGIS, 779LH</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="planningOffice" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Planning Office e.g. LONDON Old St</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="integrator" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of integrator of the scheme</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="planner" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of Planner of the scheme</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="manufacturer" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of Manufacturer</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="installer" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of Installer of the scheme</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="locality" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Locality is mapped to Exchange on BerT. e.g. WV, YMHR, BEL</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="technology" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>It is technology to be used e.g. Ethernet, SDH , WDM</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="source" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Source of change eg. Supplier</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="urgency" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Whether scheme is to be progressed in standard way or fast track e.g. Fast Track or Standard</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="bandwidth" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Bandwidth of connectivity being planned using scheme. e.g. STM-1</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="network" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The Network to which the elements planned in scheme will belong to e.g. 21CN</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="associationType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Association type</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="nou" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>NOU</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="physicalStructureName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the Physical Structure</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="logicalStructureDetails" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="name" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Name of the structure e.g. AAAS </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="structureType" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>This reflects topology in the network e.g. Ring, Cell, DXC, Artery, Tree Structure etc</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="category" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>For each logical structure we have different categories e.g. Ring can be normal or SPRING. Each category has specific functionality. e.g. Normal, SPRing</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="capacityIndicator" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>e.g. C</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="tierLevel" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Type of trafiic - 1 International traffic, 4 Customer premise etc e.g. 1, 2, 3, 4</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="schemeDrivers" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="schemeDriver" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>e.g. SDH CAPACITY GROWTH, MAJOR CUSTOMER NETWORKS</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="schemeDates" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="schemeDate" type="mnpltxn:ResourceDate" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity ResourceDate (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="associatedScheme" type="mnpltxn:Scheme" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> Refers to PBP EDM entity Scheme (Cardinality - 0..1)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="notes" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="note" type="mnpltxn:KVP" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Capture scheme related notes. Refers to PBP EDM entity KVP (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="TerminationPoint">
				<xsd:annotation>
					<xsd:documentation>A Termination Point (TP) object represents a logical abstraction of an endpoint (actual or potential) of either: A Topological Link (TL) or A Subnetwork Connection (SNC). Extends LogicalResource</xsd:documentation>
				</xsd:annotation>
				<xsd:complexContent>
					<xsd:extension base="mnpltxn:LogicalResource">
						<xsd:sequence>
							<xsd:element name="tpType" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Specifies the type of the inventory. eg. if it is a router, edge router, splitter etc.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tpModel" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Specifies the product Name/ model name.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tpVersion" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Specifies the version of the inventory.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="tpUsage" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Specifies the usage of the inventory.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="lagId" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>The id of LAG</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="lagStatus" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Lag status e.g. Lag enabled (LED), Lag disabled (LDD), Lag ceased (CSD) etc</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="directionality" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>This attribute shall represent the directionality of the signal flow at the endpoint. Refers PBP Enumeration DirectionEnum</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="isEdgePoint" type="xsd:boolean" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Indicates if the TP is an edge point of at least one subnetwork, i.e. if it is an end point of a potential inter-subnetwork topological link.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="isEquipmentProtected" type="xsd:boolean" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>The attribute indicates whether or not the TP is supported by a protected equipment.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ipPort" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>IP Port</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="reservedBy" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Name of rsouce which has already reserved this TP.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>
			<xsd:complexType name="UnvalidatedAddress">
				<xsd:annotation>
					<xsd:documentation>An unvalidated address is created when the copy of the external reference set has not yet been refreshed for all new addresses. </xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="addressLine1" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Holds Building Name or Number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressLine2" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Holds Flat Number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressLine3" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Holds Street Name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressLine4" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Holds Town name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressLine5" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Holds the name of the country</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressLine6" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Holds the postcode</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="resourceRef" type="mnpltxn:EntityRef"/>
			<xsd:element name="subNetworkProjectRef" type="mnpltxn:EntityRef"/>
			<xsd:element name="subNetworkProjectRefs">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="mnpltxn:subNetworkProjectRef" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:complexType name="Error">
				<xsd:sequence>
					<xsd:element name="errorCode" type="xsd:string" minOccurs="0"/>
					<xsd:element name="errorDescription" type="xsd:string" minOccurs="0"/>
					<xsd:element name="raisingSystem" type="xsd:string" minOccurs="0"/>
					<xsd:element name="relatedErrorList" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="relatedError" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Refers to PBP EDM entity Error (Cardinality - 0..*)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:schema>
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="https://collaborate.bt.com/svn/edm/ssp/trunk/Address.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified">
			<xs:complexType name="Address">
				<xs:annotation>
					<xs:documentation>   An Address is a structured way of locating a building or other type of premise that requires postal deliveries or people to visit it. It has no physical presence of its own but it is always related to a real-world object. As such, an address can always be associated with a Position or geographical coordinate. In Post Office terms, it is a Delivery Point, a description which enables anyone to get to the front door of the premises, either on the outside of a building or inside if the building is formaly subdivided. As a postal delivery point, it typically denotes the point of entry onto premises or a subpremise if it is an addressable part of premises. It does not include locations within the premises described by the Address.  This is another layer of information which is mastered by other platforms with an interest in the building, primarily Plan and Build (PBP) which needs to identify where installations of equipment are located.  An address is normally but not always associated with a building, e.g. a P.O. Box Address is a Virtual Address and would not have a building associated with it.
			</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="addressKey" minOccurs="0">
						<xs:annotation>
							<xs:documentation> This is BTs own published Address Key formerly known as the NAD Key which uuniquely identifies a single Address     and which is independent of any one Data Provider. 
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="12"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="postalOrganisation" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The name of organisation associated with these premises for postal deliveries. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="subBuilding" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An area within a building, e.g. Flat 1 or Floor 1 aka Sub Premises. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="building" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The name of the building, used particularly where there is no street number aka Premises. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="buildingNumber" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The number of the building or premises, its position on the street aka Thoroughfare Number. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="subStreet" minOccurs="0">
						<xs:annotation>
							<xs:documentation> When a street name exists more than once within a town, it is necessary to store additional information as subStreet to uniquely identify each one. Aka Dependent Thoroughfare. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="street" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Street name associated with an address aka Thoroughfare. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="subLocality" minOccurs="0">
						<xs:annotation>
							<xs:documentation> A locality within the main locality within which the thoroughfare or street is located aka Double Dependent Locality. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="locality" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The Locality is a recognised geographic area within the City or Town and in which the Street aka Thoroughfare is located. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="city" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The town or city within which the premises are located. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="subCountyStateProvince" minOccurs="0">
						<xs:annotation>
							<xs:documentation> A recognised, geographic area within the county, state or province defined within the same Address, e.g. a US County within a State. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="countyStateProvince" minOccurs="0">
						<xs:annotation>
							<xs:documentation> County, region, province, state etc. As a full name. Includes US and other types of federal state which should be as per ISO standard name, e.g. North Yorkshire in UK or Pennsylvania in USA. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="country" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The ISO standard full name of the country. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="countryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The ISO 2-character code for the country. . </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="postCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						National standard post or zip code, e.g. YO7 1BG in UK or 19134 for Philadelphia in the US. 
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="postCodeExtension" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						An extension of Post Code, for the US only, the Zipplus4 code which gives a more precise reading by geographic segment, i.e. block, apartment block, large postal user site etc of the Zip Code for the city.  Aka DPS or Delivery Point Sequencing. 
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="10"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="poBox" minOccurs="0">
						<xs:annotation>
							<xs:documentation> A post office box normally referred to as a PO Box or a Postal Box is a uniquely addressable lockable box located on the premises of a post office This attribute is the address of the PO Box. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="timeZone" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Time Zone for the address. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="10"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="timeZoneEstimatedFlag" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Time Zone is exact N or approximate Y or Null where no Time Zone exists . </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="dateTimeCreated" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Date when Address created. . </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="dateTimeCeased" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Date when Address ceased.  </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="dateTimeLastModified" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Date when Address last modified.   </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressType" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Allows classification of Address. Holds external classification, e.g. OS Theme. Values tbc. </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="20"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="virtualAddressFlag" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Indicates in a simple flag whether or not the address is virtual, e.g. a PO Box Address, or not.  </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="multipleoccupancyFlag" minOccurs="0">
						<xs:annotation>
							<xs:documentation> Data sourced externally which indicates whether a Postal Address delivery point is for more than one part of the premies addressed, e.g. where there are bedsist within a large house.  This is initially to be derived from the OS AL2 Record Type which has values of  Postal, Non-Postal and Multiple Occupancy.  The first two of these will be ignored.   Allowable values are  Multiple Occupancy, Null. 
</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="validationLevel" minOccurs="0">
						<xs:annotation>
							<xs:documentation> The level to which an address has been validated against an external reference source represented as a numeric code, e.g. to fully validated to premises number or name, validated to Town or City level etc etc. Derived from Global Locator validation levels which are  0=Premise number, building name, sub-building, PO box, company, street, city, post/zip code and country validated 1=Street, city, post/zip code and country validated 2=City,country and postcode is validated 3=City and country only validated 4=Country only validated.    </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="UnstructuredAddress">
				<xs:annotation>
					<xs:documentation> The Unstructured Address consists of Address Lines only.  It is used where a formal address cannot be for reasons of size or processing capabilities at the client end. </xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="addressLine1" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine2" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine3" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine4" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine5" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine6" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine7" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine8" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="addressLine9" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
						Free format text to hold any address details.
					</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:schema>
		<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:EMP="http://capabilities.nat.bt.com/xsd/EDM/EMP/CustomerSupplyOrderLine/3.0" xmlns:ns1="http://capabilities.nat.bt.com/xsd/EDM/EMP/ConsignmentLine/3.0" targetNamespace="http://capabilities.nat.bt.com/xsd/EDM/EMP/ConsignmentLine/3.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
			<import namespace="http://capabilities.nat.bt.com/xsd/EDM/EMP/CustomerSupplyOrderLine/3.0"/>
			<annotation>
				<documentation>This schema describes the Consignment Line entity maintained by the EMP.</documentation>
			</annotation>
			<complexType name="ConsignmentLine">
				<sequence>
					<element name="ConsignmentID" minOccurs="0">
						<annotation>
							<documentation>Unique number given to a specific consignment</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="3"/>
							</restriction>
						</simpleType>
					</element>
					<element name="ConsignmentLineNumber" minOccurs="0">
						<annotation>
							<documentation>Consignment line number</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="4"/>
							</restriction>
						</simpleType>
					</element>
					<element name="CustomerSupplyOrderNumber" minOccurs="0">
						<annotation>
							<documentation>Unique number or Code given to an individual Customer Supply Order</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="30"/>
							</restriction>
						</simpleType>
					</element>
					<element name="CustomerSupplyOrderLine" type="EMP:CustomerSupplyOrderLine" minOccurs="0">
						<annotation>
							<documentation>Unique line number given to each line on the order</documentation>
						</annotation>
					</element>
					<element name="ChallanNo" minOccurs="0">
						<annotation>
							<documentation>Challan No</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string"/>
						</simpleType>
					</element>
					<element name="DeliveryNo" minOccurs="0">
						<annotation>
							<documentation>Delivery No</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string"/>
						</simpleType>
					</element>
					<element name="ShipQuantity" minOccurs="0">
						<annotation>
							<documentation>Quantity</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:integer"/>
						</simpleType>
					</element>
					<element name="DeliveredQTY" minOccurs="0">
						<annotation>
							<documentation>Derived based upon successful deliveries from the order lines. Consignment_Quantity and DeliveryStatus</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:integer"/>
						</simpleType>
					</element>
					<element name="ProvidedItemId" minOccurs="0">
						<annotation>
							<documentation>Item id - the standard ASC item code</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="40"/>
							</restriction>
						</simpleType>
					</element>
					<element name="ItemSerialNumber" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Scanned serial number for this item</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="50"/>
							</restriction>
						</simpleType>
					</element>
				</sequence>
			</complexType>
		</schema>
		<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:EMP="http://capabilities.nat.bt.com/xsd/EDM/EMP/Item/V3.0" xmlns:ns1="http://capabilities.nat.bt.com/xsd/EDM/EMP/CustomerSupplyOrderLine/3.0" targetNamespace="http://capabilities.nat.bt.com/xsd/EDM/EMP/CustomerSupplyOrderLine/3.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
			<import namespace="http://capabilities.nat.bt.com/xsd/EDM/EMP/Item/V3.0"/>
			<annotation>
				<documentation>This schema describes the Customer Supply Order Line Item Entity maintained by EMP</documentation>
			</annotation>
			<complexType name="CustomerSupplyOrderLine">
				<sequence>
					<element name="CustomerSupplyOrderNumber" minOccurs="0">
						<annotation>
							<documentation>Unique number or Code given to an individual Customer Supply Order</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="30"/>
							</restriction>
						</simpleType>
					</element>
					<element name="ReturnOrderID" minOccurs="0">
						<annotation>
							<documentation>Unique number or Code generated by Returns System (EFR) for the Return Order</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:integer"/>
						</simpleType>
					</element>
					<element name="CustomerSupplyOrderLineNumber" minOccurs="0">
						<annotation>
							<documentation>Unique line number given to each line on the order</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="30"/>
							</restriction>
						</simpleType>
					</element>
					<element name="Discount" minOccurs="0">
						<annotation>
							<documentation>Value of any discounts being applied to the order line unit value.</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:float"/>
						</simpleType>
					</element>
					<element name="RRPUnitPrice" minOccurs="0">
						<annotation>
							<documentation>Standard selling price of the item.</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:float"/>
						</simpleType>
					</element>
					<element name="VATRate" minOccurs="0">
						<annotation>
							<documentation>VAT Rate</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:float"/>
						</simpleType>
					</element>
					<element name="CustomerPartReference" minOccurs="0">
						<annotation>
							<documentation>Part reference used by the customer to identify the item. Customer can place the order using this part reference and can expect all documentation to include this part reference.</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="26"/>
							</restriction>
						</simpleType>
					</element>
					<element name="ClassOfWork" minOccurs="0">
						<annotation>
							<documentation>Categorisation the purpose to which the item on the order line is going to be used.</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:string">
								<maxLength value="5"/>
							</restriction>
						</simpleType>
					</element>
					<element name="Price" minOccurs="0">
						<annotation>
							<documentation>Item sales price</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:float"/>
						</simpleType>
					</element>
					<element name="LineQuantity" minOccurs="0">
						<annotation>
							<documentation>Number of the item being order on the order line.</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:integer"/>
						</simpleType>
					</element>
					<element name="RequiredDate" minOccurs="0">
						<annotation>
							<documentation>Required date, also known as required shipment date; customer required date; customer agreed date.</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:dateTime"/>
						</simpleType>
					</element>
					<element name="RequestedShipDate" minOccurs="0">
						<annotation>
							<documentation>Requested shipment date</documentation>
						</annotation>
						<simpleType>
							<restriction base="xs:dateTime"/>
						</simpleType>
					</element>
					<element name="OUC" minOccurs="0">
