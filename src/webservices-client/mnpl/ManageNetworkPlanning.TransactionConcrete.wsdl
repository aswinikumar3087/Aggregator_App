								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="task" type="mnpltxn:NetworkTask" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Tasks part of this planning activity. Refers to PBP EDM entity - NetworkTask (0..*)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="customerAppointmentList" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="customerAppointment" type="ns2:CustomerEngineeringAppointment" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>Refers EWMP entity CustomerEngineeringAppointment (Cardinality - 0..*) </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
